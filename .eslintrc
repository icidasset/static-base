{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "process": true,

    "Handlebars": true
  },
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0
  }
}
